{
  "require": {
    "php": "5.6.*",
    "silex/silex": "*@stable",
    "solution10/config": "~1.0",
    "twig/twig": "~1.8",
    "monolog/monolog": "~1.0",
    "doctrine/dbal": "^2.5",
    "doctrine/orm": "^2.5"
  },

  "require-dev": {
    "phpunit/phpunit": "~4.1",
    "squizlabs/php_codesniffer": "~2"
  },

  "autoload": {
    "psr-4": {
      "App\\": "app/src/App"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UnitTests\\App\\": "app/tests/App"
    }
  }
}
