<phpunit>
    <filter>
        <whitelist processUncoveredFilesFromWhitelist="true">
            <directory suffix=".php">../src/</directory>
        </whitelist>
    </filter>
    <logging>
        <log type="coverage-html" target="../../html/coverage" lowUpperBound="35"
             highLowerBound="90"/>
    </logging>
</phpunit>